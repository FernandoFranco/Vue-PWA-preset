{
  "vueVersion": "2",
  "useConfigFiles": true,
  "cssPreprocessor": "dart-sass",
  "plugins": {
    "@vue/cli-plugin-babel": {
      "version": "~4.5.0"
    },
    "@vue/cli-plugin-eslint": {
      "version": "~4.5.0",
      "config": "airbnb",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "@vue/cli-plugin-pwa": {
      "version": "~4.5.0"
    },
    "@vue/cli-plugin-router": {
      "version": "~4.5.0",
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {
      "version": "~4.5.0"
    },
    "vue-cli-plugin-i18n": {
      "version": "~1.0.1",
      "locale": "en",
      "fallbackLocale": "en",
      "localeDir": "locales",
      "enableInSFC": false
    },
    "vue-cli-plugin-vuetify": {
      "version": "~2.0.8",
      "preset": "configure",
      "replaceComponents": true,
      "useTheme": true,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": true,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    }
  }
}
